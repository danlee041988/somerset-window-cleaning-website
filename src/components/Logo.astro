---
import { SITE } from 'astrowind:config';
---

<div class="flex items-center gap-3">
  <img
    src="/brand/logo.png"
    alt={SITE?.name ?? 'Logo'}
    class="logo-img h-[162px] md:h-[180px] w-auto object-contain transition-all duration-300 ease-out"
    width="500"
    height="500"
    loading="eager"
    decoding="async"
  />
</div>
